# Example .env file for cloud-sql-docker-migrator
# Fill in the values for your Google Cloud SQL instances and credentials

# Source DB
SOURCE_INSTANCE_CONNECTION_NAME=your-source-project:region:source-instance

# Target DB
TARGET_INSTANCE_CONNECTION_NAME=your-target-project:region:target-instance

# (Optional) Path to your service account key file
GOOGLE_APPLICATION_CREDENTIALS=/secrets/service-account.json
